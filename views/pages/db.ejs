<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>DB</title>
  </head>
  <body>
    <table>
      <tr>
        <td>ID</td>
        <td>Name</td>
      </tr>
      <% rows.forEach(function(r){ %>
        <tr>
          <td><%= r.id %></td>
          <td><%= r.name %></td>
        </tr>
      <% }); %>
    </table>
  </body>
</html>
